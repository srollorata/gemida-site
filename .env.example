# Copy this file to `.env` and fill in real values. Do NOT commit your `.env` file.

# Database (used by Prisma)
DATABASE_URL="postgresql://user:password@localhost:5432/gemida?schema=public"

# JWT used for server-side authentication
JWT_SECRET="replace_with_a_long_random_secret"

# UploadThing (optional)
# UPLOADTHING_SECRET="your_uploadthing_secret"
# NEXT_PUBLIC_UPLOADTHING_URL="https://uploadthing.example.com"

# App
NEXT_PUBLIC_SITE_URL="http://localhost:3000"
NODE_ENV=development
